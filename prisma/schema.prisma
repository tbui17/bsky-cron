generator client {
  provider = "prisma-client"
  output = "./generated"
}

datasource db {
  provider  = "postgresql"
}

model Post {
  id        Int       @id @default(autoincrement())
  createdAt DateTime  @default(now()) @map("created_at")
  body      String    @default("") @db.Text
  time      DateTime  @unique
  sentTime  DateTime? @map("sent_time")

  @@map("post")
}
