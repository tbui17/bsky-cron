generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Post {
  id        Int       @id @default(autoincrement())
  createdAt DateTime  @default(now()) @map("created_at")
  body      String    @default("") @db.Text
  time      DateTime  @unique
  sentTime  DateTime? @map("sent_time")

  @@map("posts")
}
